def mostrar_anios_cumplidos(edad):
    for anio in range(1, edad + 1):
        print(f"Ha cumplido {anio} {'a침o' if anio == 1 else 'a침os'}.")

def main():
    try:
        # Solicitar al usuario que ingrese su edad
        edad = int(input("Ingrese su edad: "))

        # Mostrar los a침os cumplidos por pantalla
        mostrar_anios_cumplidos(edad)

    except ValueError:
        print("Por favor, ingrese una edad v치lida.")

if __name__ == "__main__":
    main()