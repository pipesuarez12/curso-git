def mostrar_tabla_multiplicar():
    for i in range(1, 11):
        print(f"\nTabla de multiplicar del {i}:\n")
        for j in range(1, 11):
            resultado = i * j
            print(f"{i} x {j} = {resultado}")

def main():
    # Mostrar la tabla de multiplicar del 1 al 10
    mostrar_tabla_multiplicar()

if __name__ == "__main__":
    main()